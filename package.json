{
  "name": "multiple-repo-manager",
  "description": "CLI for managing multiple repositories",
  "version": "4.9.1",
  "author": "Mike Donnalley",
  "homepage": "https://github.com/mdonnalley/multiple-repo-manager",
  "repository": "mdonnalley/multiple-repo-manager",
  "bugs": "https://github.com/mdonnalley/multiple-repo-manager/issues",
  "keywords": [
    "multiple package manager",
    "package manager",
    "multiple repo manager",
    "repo manager"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@alcalzone/ink-table": "^1.1.1",
    "@inkjs/ui": "^1.0.0",
    "@inquirer/input": "^1.2.16",
    "@oclif/core": "^3.25.1",
    "@octokit/plugin-paginate-graphql": "^4.0.0",
    "@octokit/plugin-request-log": "^4.0.0",
    "cli-spinners": "^2.9.2",
    "debug": "^4.3.4",
    "ink": "^4.4.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-link": "^3.0.0",
    "ink-task-list": "^2.0.0",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.isempty": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "minimatch": "^9.0.3",
    "octokit": "^3",
    "open": "^10.0.3",
    "p-queue": "^8.0.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18",
    "@oclif/prettier-config": "^0.2.1",
    "@octokit/types": "^12.4.0",
    "@semantic-release/changelog": "^6",
    "@semantic-release/git": "^10",
    "@types/debug": "^4.1.12",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/lodash.sortby": "^4.7.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18",
    "@types/react": "^18.2.65",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "chai": "^4.3.4",
    "commitlint": "^18",
    "eslint": "^8.55.0",
    "eslint-config-oclif": "^5.1.1",
    "eslint-config-oclif-typescript": "^3.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-xo": "^0.44.0",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9",
    "lint-staged": "^15",
    "madge": "^6.1.0",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^3",
    "semantic-release": "^23",
    "sinon": "^17",
    "ts-node": "^10",
    "typescript": "^5",
    "xo": "^0.58.0"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "commitlint": "commitlint --edit $1",
    "lint-staged": "lint-staged",
    "lint": "eslint . --ext .ts",
    "postpack": "rm oclif.manifest.json",
    "posttest": "npm run lint && npm run test:circular-deps",
    "prepack": "rm -rf lib && tsc && oclif manifest && npm shrinkwrap",
    "prepare": "husky && npm run build",
    "readme": "oclif readme --no-aliases && git add README.md",
    "semantic-release": "semantic-release",
    "test:circular-deps": "madge lib/ -c",
    "test": "mocha \"test/**/*.test.ts\" --forbid-only"
  },
  "bin": {
    "multi": "bin/run.js"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/npm-shrinkwrap.json",
    "/bin/run.js"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "multi",
    "topicSeparator": " ",
    "helpOptions": {
      "hideAliasesFromRoot": true
    },
    "topics": {
      "org": {
        "description": "View information at the org level."
      },
      "task": {
        "description": "Manage local tasks."
      }
    },
    "hooks": {
      "init": "./lib/hooks/init"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": "./lib/index.js"
}
