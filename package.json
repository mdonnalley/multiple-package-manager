{
  "name": "multiple-repo-manager",
  "description": "CLI for managing multiple repositories",
  "version": "3.0.0",
  "author": "Mike Donnalley",
  "homepage": "https://github.com/mdonnalley/multiple-repo-manager",
  "repository": "mdonnalley/multiple-repo-manager",
  "bugs": "https://github.com/mdonnalley/multiple-repo-manager/issues",
  "keywords": [
    "multiple package manager",
    "package manager",
    "multiple repo manager",
    "repo manager"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@inquirer/input": "^1.2.16",
    "@oclif/core": "^3",
    "@salesforce/kit": "^3",
    "chalk": "^5.3.0",
    "js-yaml": "^4.1.0",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.isempty": "^4.4.0",
    "lodash.sortby": "^4.7.0",
    "octokit": "^3",
    "open": "^10.0.3",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.groupby": "^4.6.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/lodash.sortby": "^4.7.9",
    "@types/node": "^16.6.2",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4.3.4",
    "commitlint": "^13.1.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6",
    "lint-staged": "^11.0.0",
    "mocha": "^10",
    "oclif": "^4",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.4.5",
    "sinon": "^17",
    "ts-node": "^10",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "commitlint": "commitlint",
    "lint-staged": "lint-staged",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "postpack": "rm oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "rm -rf lib && tsc && oclif manifest .",
    "readme": "oclif readme && git add README.md",
    "reformat": "prettier --config .prettierrc.json --write './*.{js,json,md}' './**/*.{ts,json,md}'",
    "semantic-release": "semantic-release",
    "test": "mocha \"test/**/*.test.ts\" --forbid-only"
  },
  "bin": {
    "multi": "bin/run.js"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "multi",
    "topicSeparator": " "
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": "./lib/index.js"
}
