{
  "name": "multiple-package-manager",
  "description": "CLI for managing multiple node packages",
  "version": "1.0.0-beta.3",
  "author": "Mike Donnalley",
  "homepage": "https://github.com/mdonnalley/multiple-package-manager",
  "repository": "mdonnalley/multiple-package-manager",
  "bugs": "https://github.com/mdonnalley/multiple-package-manager/issues",
  "keywords": [
    "multiple package manager",
    "package manager"
  ],
  "license": "BSD-3-Clause",
  "dependencies": {
    "@oclif/core": "^0.5.31",
    "@octokit/core": "^3.5.1",
    "@salesforce/kit": "^1.5.17",
    "chalk": "^4.1.2",
    "cli-ux": "^5.6.3",
    "inquirer": "^8.1.2",
    "lodash": "^4.17.21",
    "open": "^8.2.1",
    "shelljs": "^0.8.4",
    "tslib": "^2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^13.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/inquirer": "^7.3.3",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.6.2",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "chai": "^4.3.4",
    "commitlint": "^13.1.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6",
    "lint-staged": "^11.0.0",
    "mocha": "^8.4.0",
    "oclif": "^2.0.0-main.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.4.5",
    "sinon": "^11.1.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "commitlint": "commitlint",
    "lint-staged": "lint-staged",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "postpack": "rm oclif.manifest.json",
    "posttest": "yarn run lint",
    "prepack": "rm -rf lib && tsc && oclif manifest .",
    "reformat": "prettier --config .prettierrc.json --write './*.{js,json,md}' './**/*.{ts,json,md}'",
    "semantic-release": "semantic-release",
    "test": "mocha \"test/**/*.test.ts\" --forbid-only",
    "version": "oclif readme && git add README.md"
  },
  "bin": "mpm",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mpm",
    "topicSeparator": " "
  },
  "publishConfig": {
    "access": "public"
  }
}
